<%- content_for :meta_title do -%>Add image to <%= h @owner.title %><%- end -%>

<h1><%= yield :meta_title %></h1>

<% form_for [:admin, @owner,  @image], :html => { :multipart => true, :onsubmit => "form_submitted('Uploading file and making thumbnails...')" } do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
<% end %>

