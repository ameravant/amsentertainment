<%- content_for(:title) { concat "Edit testimonial" } -%>



<h1><%= yield :title %></h1>

<% form_for :testimonial, :url => [:admin, @owner, @testimonial], :html => { :onsubmit => "form_submitted();", :method => :put, :multipart => true } do |f| %>
<%= render :partial => "form", :locals => { :f => f } %>
<% end %>
