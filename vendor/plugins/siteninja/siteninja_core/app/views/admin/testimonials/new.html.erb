<%- content_for(:title) { concat "Add testimonial" } -%>



<h1><%= yield :title %></h1>


<% form_for :testimonial, :url => [:admin, @owner, :testimonials], :html => { :onsubmit => "form_submitted();", :multipart => true } do |f| %>
<%= render :partial => "form", :locals => { :f => f } %>
<% end %>

